    "ethers": "^6.13.4",
    "gradient-string": "^3.0.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "pinia-plugin-persist": "^1.0.0",
    "pinyin-pro": "^3.25.0",
    "qs": "^6.13.0",
    "vant": "^4.9.8",
    "vue": "^3.4.29",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "@vant/auto-import-resolver": "^1.2.1",
